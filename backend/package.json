{
  "name": "skillswap-backend",
  "version": "1.0.0",
  "description": "SkillSwap backend API for AI skill extraction and matching",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "seed": "node seed.js",
    "seed:clear": "node seed.js --clear",
    "seed:large": "node seed.js --users 50 --min-connections 2 --max-connections 8 --clear"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.45.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1"
  }
}
